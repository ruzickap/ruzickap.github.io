{
  "aliveStatusCodes": [200, 999],
  "ignorePatterns": [
    {
      "pattern": "^https://img.shields.io/"
    },
    {
      "pattern": "^https://.*k01.k8s.mylabs.dev/.*"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/assets/",
      "replacement": "../../assets/"
    }
  ],
  "retryCount": 5,
  "retryOn429": true,
  "fallbackRetryDelay": "30s"
}
